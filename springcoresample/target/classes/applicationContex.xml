<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans.xsd">
       <!-- setter injection-->
       <bean id="studentBean" class="com.tnsif.springcoresample.Student">
       
       <property name="name" value="Shalini"></property>
       
       </bean>
       
       <bean id="collegeBean" class="com.tnsif.springcoresample.College">
       
       <property name="student" ref="studentBean"></property></bean>
       
</beans>